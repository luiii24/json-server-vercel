{
  "functions": {
    "api/server.js": {
      "memory": 1024,
      "includeFiles": "db.json"
    }
  },
  "rewrites": [
    {
      "source": "/o",
      "has": [
        {
          "type": "header",
          "key": "x-lui",
          "value": "ukl"
        }
      ],
      "destination": "api/server.js"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "query",
          "key": "authorized"
        }
      ],
      "headers": [
        {
          "key": "x-authorized",
          "value": "true"
        }
      ]
    }
  ]
  "redirects": [
    { "source": "/l", "destination": "https://lui.biz.id", "permanent": false }
  ]
}
