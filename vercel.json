{
  "functions": {
    "api/server.js": {
      "memory": 1024,
      "includeFiles": "db.json"
    }
  },
  "rewrites": [
    {
      "source": "/api",
      "destination": "api/server.js"
    }
  ],
  "headers": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "query",
          "key": "authorized"
        }
      ],
      "headers": [
        {
          "key": "x-ukk",
          "value": "linggis"
        }
      ]
    }
  ]
}
