{
  "functions": {
    "api/server.js": {
      "memory": 1024,
      "includeFiles": "db.json"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "query",
          "value": "ukk"
        },
        {
          "type": "header",
          "key": "x-ukk",
          "value": "nnn"
        }
      ],
      "destination": "api/server.js"
    }
  ],
}
